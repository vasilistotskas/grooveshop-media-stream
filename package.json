{
  "name": "@vasilistotskas/grooveshop-media-stream",
  "version": "2.28.0",
  "private": false,
  "type": "module",
  "description": "Grooveshop Media Stream",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasilistotskas/grooveshop-media-stream.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasilistotskas/grooveshop-media-stream/issues"
  },
  "homepage": "https://github.com/vasilistotskas/grooveshop-media-stream#readme",
  "files": [
    "build/dist/**/*"
  ],
  "imports": {
    "#microservice*": "./src/MediaStream/*.ts",
    "#storage*": "./var/*"
  },
  "scripts": {
    "prebuild": "rimraf build/dist && node scripts/inject-version.cjs",
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --tsc --watch",
    "dev:all": "ncu -u && pnpm i && pnpm run build && pnpm run lint && pnpm run dev",
    "debug": "nest start --debug --watch",
    "prod": "node build/dist/main",
    "repl": "nest start --watch --entryFile repl",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "vitest run",
    "test:e2e": "vitest run --config src/test/vitest.e2e.ts",
    "test:coverage": "vitest run --coverage && coveralls < coverage/lcov.info",
    "cache:clear": "node scripts/clear-cache.cjs",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@nestjs/axios": "4.0.1",
    "@nestjs/bull": "11.0.4",
    "@nestjs/common": "11.1.12",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.12",
    "@nestjs/mapped-types": "2.1.0",
    "@nestjs/platform-express": "11.1.12",
    "@nestjs/schedule": "6.1.0",
    "@nestjs/serve-static": "5.0.4",
    "@nestjs/terminus": "11.0.0",
    "@nestjs/throttler": "6.5.0",
    "@types/compression": "1.8.1",
    "axios": "1.13.2",
    "bull": "4.16.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "compression": "1.8.1",
    "express": "5.2.1",
    "helmet": "8.1.0",
    "ioredis": "5.9.2",
    "module-alias": "2.2.3",
    "node-cache": "5.1.2",
    "prom-client": "15.1.3",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.1.2",
    "rxjs": "7.8.2",
    "sharp": "0.34.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "7.0.1",
    "@nestjs/cli": "11.0.16",
    "@nestjs/schematics": "11.0.9",
    "@nestjs/testing": "11.1.12",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "12.0.2",
    "@semantic-release/npm": "13.1.3",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@vitest/coverage-v8": "4.0.17",
    "@swc/cli": "0.7.10",
    "@swc/core": "1.15.8",
    "@types/express": "5.0.6",
    "@types/node": "25.0.9",
    "@types/supertest": "6.0.3",
    "@vitest/eslint-plugin": "1.6.6",
    "coveralls-next": "6.0.1",
    "eslint": "9.39.2",
    "semantic-release": "25.0.2",
    "source-map-support": "0.5.21",
    "supertest": "7.2.2",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.3",
    "vitest": "4.0.17",
    "unplugin-swc": "1.5.9"
  },
  "publishConfig": {
    "@vasilistotskas:registry": "https://npm.pkg.github.com"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "dist",
              "label": "Distribution"
            }
          ]
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false,
          "tarballDir": "dist"
        }
      ]
    ]
  },
  "packageManager": "pnpm@10.28.0"
}
